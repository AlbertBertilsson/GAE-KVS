<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GAEKVS</servlet-name>
		<servlet-class>nu.albert.gaekvs.GAEKVSServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>INIT</servlet-name>
		<servlet-class>nu.albert.gaekvs.INITServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TEST</servlet-name>
		<servlet-class>nu.albert.gaekvs.TESTServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GAEKVS</servlet-name>
		<url-pattern>/gaekvs/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>INIT</servlet-name>
		<url-pattern>/init/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TEST</servlet-name>
		<url-pattern>/test/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
